      <rule id="DecisionRule_15">
        <inputEntry id="UnaryTests_LeadSource_15">
          <text>"COLD_OUTREACH"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_CompanySize_15">
          <text>"STARTUP","SMALL"</text>
