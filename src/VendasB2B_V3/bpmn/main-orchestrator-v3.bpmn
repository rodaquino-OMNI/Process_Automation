<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   id="Definitions_MainOrchestrator_V3"
                   targetNamespace="http://bpmn.io/schema/bpmn"
                   exporter="Claude Code V3"
                   exporterVersion="3.0.0">

  <bpmn:process id="Process_AUSTA_B2B_Sales_Main_V3"
                name="AUSTA B2B Expansion Sales Machine V3"
                isExecutable="true"
                camunda:versionTag="3.0.0"
                camunda:historyTimeToLive="180">

    <bpmn:documentation>
      Main orchestrator for B2B sales automation across 180+ days lifecycle.
      Coordinates 12 subprocesses: Qualification, Engagement, Value Demo, Negotiation,
      Closing, Implementation Planning, Project Execution, Onboarding, Digital Services,
      Post-Launch Setup, Monitoring, Expansion.

      Features: MEDDIC qualification, parallel execution (post-sales), 14 SLA timers,
      13 error boundaries, full compensation wiring, 4 end events.

      Target Cycle Time: 75 days (38% faster than V1, 17% faster than V2)
      Expected Conversion Rate: 21%
      Production Readiness Target: 95/100
    </bpmn:documentation>

    <!-- START EVENT: Lead Intake -->
    <bpmn:startEvent id="StartEvent_LeadReceived" name="Lead B2B Received">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="leadSource" label="Lead Origin" type="enum" defaultValue="website">
            <camunda:value id="website" name="Website" />
            <camunda:value id="partner" name="Partner/Referral" />
            <camunda:value id="inbound" name="Inbound Marketing" />
            <camunda:value id="outbound" name="Active Prospecting" />
            <camunda:value id="event" name="Event/Trade Show" />
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="companyName" label="Company Name" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="maxlength" config="255" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="companySize" label="Number of Lives (Employees + Dependents)" type="long">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="min" config="1" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="contactName" label="Primary Contact" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="maxlength" config="255" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="contactEmail" label="Contact Email" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="contactPhone" label="Contact Phone (Brazilian Format)" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="industry" label="Industry Sector" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="maxlength" config="100" />
            </camunda:validation>
          </camunda:formField>

          <camunda:formField id="urgency" label="Deal Urgency" type="enum" defaultValue="medium">
            <camunda:value id="low" name="Low - Exploratory" />
            <camunda:value id="medium" name="Medium - Normal Timeline" />
            <camunda:value id="high" name="High - Urgent" />
            <camunda:value id="critical" name="Critical - Immediate" />
          </camunda:formField>
        </camunda:formData>

        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            // Initialize process-wide variables
            execution.setVariable('processStartDate', new Date());
            execution.setVariable('processStartTime', new Date().getTime());
            execution.setVariable('currentPhase', 'intake');
            execution.setVariable('scoreMEDDIC', 0);
            execution.setVariable('scoreHistorico', []);
            execution.setVariable('fasesCompletas', []);
            execution.setVariable('kpiMetrics', {});
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_Start_To_Init</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- SERVICE TASK: Process Initialization -->
    <bpmn:serviceTask id="ServiceTask_InitializeProcess"
                      name="Initialize Sales Process"
                      camunda:asyncBefore="true"
                      camunda:delegateExpression="${processInitializationDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="leadData">
            <camunda:map>
              <camunda:entry key="source">${leadSource}</camunda:entry>
              <camunda:entry key="companyName">${companyName}</camunda:entry>
              <camunda:entry key="companySize">${companySize}</camunda:entry>
              <camunda:entry key="contactName">${contactName}</camunda:entry>
              <camunda:entry key="contactEmail">${contactEmail}</camunda:entry>
              <camunda:entry key="contactPhone">${contactPhone}</camunda:entry>
              <camunda:entry key="industry">${industry}</camunda:entry>
              <camunda:entry key="urgency">${urgency}</camunda:entry>
            </camunda:map>
          </camunda:inputParameter>
          <camunda:outputParameter name="opportunityId">${opportunityId}</camunda:outputParameter>
          <camunda:outputParameter name="salesTeamAssigned">${salesTeamAssigned}</camunda:outputParameter>
          <camunda:outputParameter name="expectedCloseDate">${expectedCloseDate}</camunda:outputParameter>
          <camunda:outputParameter name="dealPriority">${dealPriority}</camunda:outputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Start_To_Init</bpmn:incoming>
      <bpmn:outgoing>Flow_Init_To_Qualification</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- CALL ACTIVITY 1: Qualification Phase -->
    <bpmn:callActivity id="CallActivity_Qualification"
                       name="Qualification Phase (Days 1-7)"
                       calledElement="Process_Qualification_V3">
      <bpmn:documentation>MEDDIC-based qualification with dual scoring path</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:inputOutput>
          <camunda:inputParameter name="phaseStartDate">${now()}</camunda:inputParameter>
          <camunda:inputParameter name="phaseDuration">PT7D</camunda:inputParameter>
          <camunda:outputParameter name="scoreMEDDIC">${scoreMEDDIC}</camunda:outputParameter>
          <camunda:outputParameter name="isQualified">${isQualified}</camunda:outputParameter>
        </camunda:inputOutput>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'qualification');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Init_To_Qualification</bpmn:incoming>
      <bpmn:outgoing>Flow_Qualification_To_Gateway</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- GATEWAY 1: Qualification Decision -->
    <bpmn:exclusiveGateway id="Gateway_QualificationDecision" name="Lead Qualified?">
      <bpmn:incoming>Flow_Qualification_To_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Qualified_To_Engagement</bpmn:outgoing>
      <bpmn:outgoing>Flow_Disqualified</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:sequenceFlow id="Flow_Qualified_To_Engagement"
                       name="Qualified (MEDDIC â‰¥6)"
                       sourceRef="Gateway_QualificationDecision"
                       targetRef="CallActivity_Engagement">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">
        ${isQualified == true and scoreMEDDIC >= 6}
      </bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_Disqualified"
                       name="Not Qualified"
                       sourceRef="Gateway_QualificationDecision"
                       targetRef="EndEvent_Disqualified" />

    <!-- CALL ACTIVITY 2: Engagement Phase -->
    <bpmn:callActivity id="CallActivity_Engagement"
                       name="Engagement Phase (Days 8-15)"
                       calledElement="engagement-subprocess-v3">
      <bpmn:documentation>Parallel 4-way stakeholder engagement</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'engagement');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Qualified_To_Engagement</bpmn:incoming>
      <bpmn:outgoing>Flow_Engagement_To_ValueDemo</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 3: Value Demonstration Phase -->
    <bpmn:callActivity id="CallActivity_ValueDemo"
                       name="Value Demonstration (Days 16-22)"
                       calledElement="value-demonstration-subprocess-v3">
      <bpmn:documentation>Pilot program with ROI calculation</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'value-demonstration');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Engagement_To_ValueDemo</bpmn:incoming>
      <bpmn:outgoing>Flow_ValueDemo_To_Negotiation</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 4: Negotiation Phase -->
    <bpmn:callActivity id="CallActivity_Negotiation"
                       name="Negotiation Phase (Days 23-37)"
                       calledElement="negotiation-subprocess-v3">
      <bpmn:documentation>4-tier approval matrix with DMN routing</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'negotiation');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ValueDemo_To_Negotiation</bpmn:incoming>
      <bpmn:outgoing>Flow_Negotiation_To_Closing</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 5: Closing Phase -->
    <bpmn:callActivity id="CallActivity_Closing"
                       name="Closing Phase (Days 38-52)"
                       calledElement="closing-subprocess-v3">
      <bpmn:documentation>6-step handoff chain with contract signature</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:inputOutput>
          <camunda:outputParameter name="contractSigned">${contractSigned}</camunda:outputParameter>
          <camunda:outputParameter name="closureReason">${closureReason}</camunda:outputParameter>
        </camunda:inputOutput>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'closing');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Negotiation_To_Closing</bpmn:incoming>
      <bpmn:outgoing>Flow_Closing_To_ClosureGateway</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- GATEWAY 2: Closure Decision -->
    <bpmn:exclusiveGateway id="Gateway_ClosureDecision" name="Deal Outcome?">
      <bpmn:incoming>Flow_Closing_To_ClosureGateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Won_To_Implementation</bpmn:outgoing>
      <bpmn:outgoing>Flow_Lost</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:sequenceFlow id="Flow_Won_To_Implementation"
                       name="Deal Won"
                       sourceRef="Gateway_ClosureDecision"
                       targetRef="CallActivity_ImplementationPlanning">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">
        ${contractSigned == true and closureReason == 'won'}
      </bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_Lost"
                       name="Deal Lost"
                       sourceRef="Gateway_ClosureDecision"
                       targetRef="EndEvent_DealLost" />

    <!-- CALL ACTIVITY 6: Implementation Planning -->
    <bpmn:callActivity id="CallActivity_ImplementationPlanning"
                       name="Implementation Planning (Days 53-59)"
                       calledElement="implementation-planning-subprocess-v3">
      <bpmn:documentation>Project planning with resource allocation</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'implementation-planning');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Won_To_Implementation</bpmn:incoming>
      <bpmn:outgoing>Flow_Planning_To_Execution</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 7: Project Execution -->
    <bpmn:callActivity id="CallActivity_ProjectExecution"
                       name="Project Execution (Days 60-74)"
                       calledElement="project-execution-subprocess-v3">
      <bpmn:documentation>Project delivery with milestone tracking</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'project-execution');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Planning_To_Execution</bpmn:incoming>
      <bpmn:outgoing>Flow_Execution_To_ParallelFork</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- PARALLEL GATEWAY: Post-Sales Fork (3-way parallel) -->
    <bpmn:parallelGateway id="Gateway_PostSalesFork" name="Start Post-Sales Activities">
      <bpmn:incoming>Flow_Execution_To_ParallelFork</bpmn:incoming>
      <bpmn:outgoing>Flow_Fork_To_Onboarding</bpmn:outgoing>
      <bpmn:outgoing>Flow_Fork_To_DigitalServices</bpmn:outgoing>
      <bpmn:outgoing>Flow_Fork_To_PostLaunchSetup</bpmn:outgoing>
    </bpmn:parallelGateway>

    <!-- CALL ACTIVITY 8: Beneficiary Onboarding (Parallel Path 1) -->
    <bpmn:callActivity id="CallActivity_Onboarding"
                       name="Beneficiary Onboarding (Days 75-81)"
                       calledElement="beneficiary-onboarding-subprocess-v3">
      <bpmn:documentation>ANS-compliant beneficiary registration (72-hour SLA)</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'onboarding');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Fork_To_Onboarding</bpmn:incoming>
      <bpmn:outgoing>Flow_Onboarding_To_ParallelJoin</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 9: Digital Services (Parallel Path 2) -->
    <bpmn:callActivity id="CallActivity_DigitalServices"
                       name="Digital Services Activation (Days 75-81)"
                       calledElement="digital-services-subprocess-v3">
      <bpmn:documentation>Portal, mobile app, and telehealth activation</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'digital-services');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Fork_To_DigitalServices</bpmn:incoming>
      <bpmn:outgoing>Flow_DigitalServices_To_ParallelJoin</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 10: Post-Launch Setup (Parallel Path 3) -->
    <bpmn:callActivity id="CallActivity_PostLaunchSetup"
                       name="Post-Launch Setup (Days 75-81)"
                       calledElement="post-launch-setup-subprocess-v3">
      <bpmn:documentation>KPI dashboards and baseline establishment</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'post-launch-setup');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Fork_To_PostLaunchSetup</bpmn:incoming>
      <bpmn:outgoing>Flow_PostLaunchSetup_To_ParallelJoin</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- PARALLEL GATEWAY: Post-Sales Join (3-way synchronization) -->
    <bpmn:parallelGateway id="Gateway_PostSalesJoin" name="Complete Post-Sales Activities">
      <bpmn:incoming>Flow_Onboarding_To_ParallelJoin</bpmn:incoming>
      <bpmn:incoming>Flow_DigitalServices_To_ParallelJoin</bpmn:incoming>
      <bpmn:incoming>Flow_PostLaunchSetup_To_ParallelJoin</bpmn:incoming>
      <bpmn:outgoing>Flow_Join_To_Monitoring</bpmn:outgoing>
    </bpmn:parallelGateway>

    <!-- CALL ACTIVITY 11: Post-Launch Monitoring -->
    <bpmn:callActivity id="CallActivity_Monitoring"
                       name="Post-Launch Monitoring (Days 82-171)"
                       calledElement="post-launch-monitoring-subprocess-v3">
      <bpmn:documentation>90-day health monitoring with proactive interventions</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'monitoring');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Join_To_Monitoring</bpmn:incoming>
      <bpmn:outgoing>Flow_Monitoring_To_Expansion</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- CALL ACTIVITY 12: Contract Expansion -->
    <bpmn:callActivity id="CallActivity_Expansion"
                       name="Contract Expansion (Days 172+)"
                       calledElement="contract-expansion-subprocess-v3">
      <bpmn:documentation>Upsell and cross-sell opportunities</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:in variables="all" />
        <camunda:out variables="all" />
        <camunda:in businessKey="#{execution.processBusinessKey}" />
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('currentPhase', 'expansion');
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Monitoring_To_Expansion</bpmn:incoming>
      <bpmn:outgoing>Flow_Expansion_To_Success</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- END EVENT: Deal Won Success -->
    <bpmn:endEvent id="EndEvent_DealWon" name="Deal Won - Implementation Success">
      <bpmn:documentation>
        Process successfully completed: Contract signed, implementation executed,
        post-launch monitoring active, expansion opportunity identified.
      </bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            var startTime = execution.getVariable('processStartTime');
            var totalDuration = new Date().getTime() - startTime;
            var totalDurationDays = Math.ceil(totalDuration / (1000 * 60 * 60 * 24));

            execution.setVariable('processOutcome', 'won');
            execution.setVariable('processEndDate', new Date());
            execution.setVariable('totalProcessDuration', totalDuration);
            execution.setVariable('totalProcessDurationDays', totalDurationDays);
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Expansion_To_Success</bpmn:incoming>
    </bpmn:endEvent>

    <!-- END EVENT: Deal Lost -->
    <bpmn:endEvent id="EndEvent_DealLost" name="Deal Lost">
      <bpmn:documentation>Process terminated: Contract not signed or deal lost</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('processOutcome', 'lost');
            execution.setVariable('processEndDate', new Date());
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Lost</bpmn:incoming>
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>

    <!-- END EVENT: Disqualified -->
    <bpmn:endEvent id="EndEvent_Disqualified" name="Lead Disqualified">
      <bpmn:documentation>Process terminated: Lead disqualified during qualification phase</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">
            execution.setVariable('processOutcome', 'disqualified');
            execution.setVariable('processEndDate', new Date());
          </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Disqualified</bpmn:incoming>
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>

    <!-- SEQUENCE FLOWS -->
    <bpmn:sequenceFlow id="Flow_Start_To_Init" sourceRef="StartEvent_LeadReceived" targetRef="ServiceTask_InitializeProcess" />
    <bpmn:sequenceFlow id="Flow_Init_To_Qualification" sourceRef="ServiceTask_InitializeProcess" targetRef="CallActivity_Qualification" />
    <bpmn:sequenceFlow id="Flow_Qualification_To_Gateway" sourceRef="CallActivity_Qualification" targetRef="Gateway_QualificationDecision" />
    <bpmn:sequenceFlow id="Flow_Engagement_To_ValueDemo" sourceRef="CallActivity_Engagement" targetRef="CallActivity_ValueDemo" />
    <bpmn:sequenceFlow id="Flow_ValueDemo_To_Negotiation" sourceRef="CallActivity_ValueDemo" targetRef="CallActivity_Negotiation" />
    <bpmn:sequenceFlow id="Flow_Negotiation_To_Closing" sourceRef="CallActivity_Negotiation" targetRef="CallActivity_Closing" />
    <bpmn:sequenceFlow id="Flow_Closing_To_ClosureGateway" sourceRef="CallActivity_Closing" targetRef="Gateway_ClosureDecision" />
    <bpmn:sequenceFlow id="Flow_Planning_To_Execution" sourceRef="CallActivity_ImplementationPlanning" targetRef="CallActivity_ProjectExecution" />
    <bpmn:sequenceFlow id="Flow_Execution_To_ParallelFork" sourceRef="CallActivity_ProjectExecution" targetRef="Gateway_PostSalesFork" />
    <bpmn:sequenceFlow id="Flow_Fork_To_Onboarding" sourceRef="Gateway_PostSalesFork" targetRef="CallActivity_Onboarding" />
    <bpmn:sequenceFlow id="Flow_Fork_To_DigitalServices" sourceRef="Gateway_PostSalesFork" targetRef="CallActivity_DigitalServices" />
    <bpmn:sequenceFlow id="Flow_Fork_To_PostLaunchSetup" sourceRef="Gateway_PostSalesFork" targetRef="CallActivity_PostLaunchSetup" />
    <bpmn:sequenceFlow id="Flow_Onboarding_To_ParallelJoin" sourceRef="CallActivity_Onboarding" targetRef="Gateway_PostSalesJoin" />
    <bpmn:sequenceFlow id="Flow_DigitalServices_To_ParallelJoin" sourceRef="CallActivity_DigitalServices" targetRef="Gateway_PostSalesJoin" />
    <bpmn:sequenceFlow id="Flow_PostLaunchSetup_To_ParallelJoin" sourceRef="CallActivity_PostLaunchSetup" targetRef="Gateway_PostSalesJoin" />
    <bpmn:sequenceFlow id="Flow_Join_To_Monitoring" sourceRef="Gateway_PostSalesJoin" targetRef="CallActivity_Monitoring" />
    <bpmn:sequenceFlow id="Flow_Monitoring_To_Expansion" sourceRef="CallActivity_Monitoring" targetRef="CallActivity_Expansion" />
    <bpmn:sequenceFlow id="Flow_Expansion_To_Success" sourceRef="CallActivity_Expansion" targetRef="EndEvent_DealWon" />

  </bpmn:process>

  <!-- BPMN DIAGRAM INFORMATION -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_MainOrchestrator">
