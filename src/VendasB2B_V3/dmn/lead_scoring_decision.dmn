<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" id="LeadScoringDecision" name="Lead Scoring Decision" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.20.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.4.0">
  <decision id="Decision_LeadScoring" name="Lead Scoring Decision">
    <decisionTable id="DecisionTable_LeadScoring" hitPolicy="FIRST">
      <input id="Input_LeadSource" label="Lead Source">
        <inputExpression id="InputExpression_LeadSource" typeRef="string">
          <text>leadSource</text>
        </inputExpression>
        <inputValues id="UnaryTests_LeadSource">
          <text>"REFERRAL","WEBSITE","TRADE_SHOW","COLD_OUTREACH","PARTNER","EXISTING_CLIENT","MARKETING_CAMPAIGN"</text>
        </inputValues>
      </input>
      <input id="Input_CompanySize" label="Company Size">
        <inputExpression id="InputExpression_CompanySize" typeRef="string">
          <text>companySize</text>
        </inputExpression>
        <inputValues id="UnaryTests_CompanySize">
          <text>"ENTERPRISE","LARGE","MEDIUM","SMALL","STARTUP"</text>
        </inputValues>
      </input>
      <input id="Input_Industry" label="Industry">
        <inputExpression id="InputExpression_Industry" typeRef="string">
          <text>industry</text>
        </inputExpression>
        <inputValues id="UnaryTests_Industry">
          <text>"TECHNOLOGY","FINANCE","HEALTHCARE","MANUFACTURING","RETAIL","EDUCATION","OTHER"</text>
        </inputValues>
      </input>
      <input id="Input_Urgency" label="Urgency Level">
        <inputExpression id="InputExpression_Urgency" typeRef="string">
          <text>urgency</text>
        </inputExpression>
        <inputValues id="UnaryTests_Urgency">
          <text>"IMMEDIATE","HIGH","MEDIUM","LOW"</text>
        </inputValues>
      </input>
      <input id="Input_EstimatedValue" label="Estimated Value">
        <inputExpression id="InputExpression_EstimatedValue" typeRef="number">
          <text>estimatedValue</text>
        </inputExpression>
      </input>
      <output id="Output_LeadScore" label="Lead Score" name="leadScore" typeRef="number" />
      <output id="Output_Priority" label="Priority" name="priority" typeRef="string">
        <outputValues id="UnaryTests_Priority">
          <text>"HIGH","MEDIUM","LOW"</text>
        </outputValues>
      </output>
      <output id="Output_AutoAssign" label="Auto Assign" name="autoAssign" typeRef="boolean" />

      <!-- Rule 1: Hot Lead - Referral + Enterprise + High Value + Immediate -->
      <rule id="DecisionRule_1">
        <inputEntry id="UnaryTests_LeadSource_1">
          <text>"REFERRAL"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_CompanySize_1">
          <text>"ENTERPRISE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Industry_1">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_Urgency_1">
          <text>"IMMEDIATE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_EstimatedValue_1">
          <text>&gt;= 500000</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_LeadScore_1">
          <text>95</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_Priority_1">
          <text>"HIGH"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_AutoAssign_1">
          <text>true</text>
        </outputEntry>
      </rule>

      <!-- Rule 2: Very High Priority - Existing Client + Large + Tech/Finance -->
